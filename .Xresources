! bright_green
URxvt.color10:          #70d670
! yellow
URxvt.color3:           #fff04e
! bright_yellow
URxvt.color11:          #ffffda
! blue
URxvt.color4:           #6699ff
! bright_blue
URxvt.color12:          #7AC2FF
! magenta
